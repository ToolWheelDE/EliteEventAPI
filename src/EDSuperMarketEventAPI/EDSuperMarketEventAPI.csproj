<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>ToolWheel</RootNamespace>
    <Authors>toolwheel.de</Authors>
    <Company>ToolWheel.de</Company>
    <Description>Elite Dangerous Event API

EDSuperMarketEventAPI is a powerful event-based API for reading the game actions of Elite Dangerous.</Description>
    <PackageProjectUrl>https://github.com/ToolWheelDE/EliteEventAPI</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ToolWheelDE/EliteEventAPI</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>0.14.0-beta</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReleaseNotes>This is a beta version.</PackageReleaseNotes>
    <AssemblyVersion>0.14.0.0</AssemblyVersion>
    <FileVersion>0.14.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
